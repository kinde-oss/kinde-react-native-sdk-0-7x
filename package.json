{
    "name": "@kinde-oss/react-native-sdk-0-7x",
    "version": "2.1.0",
    "description": "Kinde React Native SDK for authentication",
    "license": "MIT",
    "homepage": "https://kinde.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/kinde-oss/kinde-react-native-sdk-0-7x"
    },
    "bugs": "https://github.com/kinde-oss/kinde-react-native-sdk-0-7x",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "vite build",
        "prepare": "pnpm run build && husky install",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "lint": "eslint . && prettier --check .",
        "lint:fix": "eslint --fix . && prettier --write .",
        "release": "release-it"
    },
    "author": {
        "name": "Kinde",
        "email": "engineering@kinde.com",
        "url": "https://kinde.com"
    },
    "browser": {
        "fs": false
    },
    "keywords": [
        "Kinde",
        "login",
        "Authorization Code Grant Flow",
        "PKCE",
        "Single Page Application authentication",
        "React Native authentication"
    ],
    "dependencies": {
        "@kinde/js-utils": "^0.18.3",
        "crypto-js": "^4.2.0",
        "jwt-decode": "^3.1.2",
        "react-native-app-auth": "^8.0.0",
        "react-native-get-random-values": "^1.11.0",
        "react-native-keychain": "^8.0.0",
        "url-parse": "^1.5.10"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.2.2",
        "@types/node": "^18.11.7",
        "@types/react-native": "^0.70.6",
        "@types/react-native-get-random-values": "^1.8.2",
        "@types/url-parse": "^1.4.8",
        "@typescript-eslint/eslint-plugin": "^8.20.0",
        "@typescript-eslint/parser": "^8.20.0",
        "@vitest/coverage-v8": "2.1.9",
        "@vitest/ui": "^2.1.8",
        "eslint": "^8.26.0",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "prettier": "^2.7.1",
        "react": "18.1.0",
        "react-native": "0.70.8",
        "typescript": "^4.9.5",
        "vite": "^5.4.11",
        "vite-plugin-dts": "^4.3.0",
        "vitest": "^2.1.8"
    },
    "peerDependencies": {
        "react-native": ">= 0.70",
        "react-native-app-auth": ">= 8.0",
        "react-native-get-random-values": ">= 1.10",
        "react-native-keychain": ">= 8.0"
    },
    "files": [
        "dist"
    ],
    "lint-staged": {
        "**/*.{js,jsx,ts,tsx}": [
            "npx prettier --write .",
            "npx eslint --fix"
        ]
    },
    "packageManager": "pnpm@9.15.4"
}
